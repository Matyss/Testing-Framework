<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <listeners>
        <listener class-name="resources.Listeners" />
    </listeners>
    <test verbose="2" preserve-order="true" name="Login user">
        <classes>
            <class name="udemyCourse.HomePage">
                <methods>
                    <include name="rootPageNavigation"/>
                    <include name="clickOnLogin"/>
                    <include name="loginUser"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>